    <!--dialog theme-->
    -<style parent="android:Theme.Dialog" name="customDialogTheme"><item name="android:windowFrame">@null</item><item name="android:windowBackground">@android:color/transparent</item><item name="android:windowIsFloating">true</item></style>

</resources>
